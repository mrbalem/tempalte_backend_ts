{
  "name": "template-api-rest",
  "version": "1.0.0",
  "description": "Template use for API rest node typescript.",
  "main": "src/server.js",
  "scripts": {
    "setup": "npm install && npm update",
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "ts-node-dev --transpileOnly ./src/server.ts",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "tsc && node ./build/src/server.js"
  },
  "keywords": [
    "express",
    "nodejs",
    "typescript"
  ],
  "license": "ARTISTIC-2.0",
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.1",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "helmet": "^3.21.2",
    "http-status": "^1.3.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "rotating-file-stream": "^2.1.3",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^3.6.3"
  },
  "_moduleAliases": {
    "@root": "src",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@util": "src/util",
    "@routes": "src/routes"
  }
}
